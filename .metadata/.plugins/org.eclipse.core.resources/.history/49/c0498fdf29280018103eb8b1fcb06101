public class Q5 {
	public static void main(String[] args) {
		
		adressToInt("www.google.com");
		
	}
	private static int adressToInt(String adress){
		
		System.out.println(adress);
		
		String tab[] = adress.split(Pattern.quote("."));
		int lenght;
		for(int i=0; i<tab.length; i++){
			System.out.println(tab[i]+" Ã  une longueur de: "tab[i].length());
		}
		
		return 0;
	}

}
