package ex2;

public class Q5 {
	public static void main(String[] args) {
		
		adressToInt("www.google.com");
		
	}
	private static int adressToInt(String adress){
		
		String tab[] = adress.split("".");
		int lenght;
		for(int i=0; i<tab.length; i++){
			System.out.println(tab[i].length());
		}
		
		return 0;
	}

}
